def greet(name):
  gr = "hello"+ name 
  return gr 
a = greet("vivek")
print(a)



def factorial(n):
  if(n==0 or n==1):
    return 1
  return n * factorial(n-1)

n = int(input("enter the number:"))
print(factorial(n))



def gretest(a,b,c):
  if(a>b and a>c):
    return a

  elif(b>a and b>c):
    return b

  elif(c>a and c>b):
    return c



a = 15
b = 24
c = 3

print(gretest(a,b,c))




def pattern(n):
  if (n==0):
    return
  print("*" *n)
  pattern(n-1)
pattern(5)




def inch_to_cm(inch):
  return inch*2.54

n = int(input("enter the value in inch: "))

print(f"the value in cm is {inch_to_cm(n)}")




def multiply(n):
  for i in range(1,11):
     print(f"{n} x {i} = {n*i}")
    
multiply(5)


