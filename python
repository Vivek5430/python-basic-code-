print(marks.get("panchal"))
print(marks)




#set

s1 = {1,2,3,4,5,6,85,89,65}
s2 = {45,85,75,41,23,63,96}

s1.remove(5)
print(s1.union(s2))
print(s1.intersection(s2))
print(len(s1))
print(s2)




